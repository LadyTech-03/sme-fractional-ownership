{
    "compilerOptions": {
      "target": "ES2020",
      "module": "CommonJS",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./dist",
      "rootDir": "./src",
      "resolveJsonModule": true,
      "lib": ["es2020"]
    },
    "include": [
      "src/**/*"
    ]
  }
  